 "html": "<style>\n\t.print-format table, .print-format tr, \n\t.print-format td, .print-format div, .print-format p {\n\t\tfont-family: Tahoma, sans-serif;\n\t\tline-height: 150%;\n\t\tvertical-align: middle;\n\t}\n\t@media screen {\n\t\t.print-format {\n\t\t\twidth: 4in;\n\t\t\tpadding: 0.25in;\n\t\t\tmin-height: 8in;\n\t\t}\n\t}\n</style>\n\n{% if letter_head %}\n    {{ letter_head }}\n{% endif %}\n\n<center><b>\n\t{{ doc.company }}<br>\n\t{{ _(\"Orden No\") }}: {{ doc.name }} {{ _(\" ---> \") }}\n\t{{ _(\"Mesa\") }}: {{ doc.table_description }} <br>\n\t{{ _(\"Mozo\") }}: {{ frappe.get_doc('User', doc.owner).full_name }}\n</b>\n\n<table class=\"table table-condensed cart no-border\">\n\t<thead>\n\t\t<tr>\n\t\t\t<th width=\"65%\"><b>{{ _(\"Item\") }}</b></th>\n\t\t\t<th width=\"25%\"><b>{{ _(\"Center\") }}</b></th>\n\t\t\t<th width=\"10%\" class=\"text-right\"><b>{{ _(\"Qty\") }}</b></th>\n\t\t</tr>\n\t</thead>\n\t<tbody>\n\t\t{%- for item in doc.entry_items -%}\n\t\t<tr>\n\t\t    {%- if item.status == \"Sent\" -%}\n\t\t\t<td>\n\t\t\t\t{{ item.item_code }} \n\t\t\t\t{%- if item.item_name != item.item_code -%}: {{ item.item_name }}\n\t\t\t\t{%- endif -%}\n\t\t\t\t{%- if item.serial_no -%}\n\t\t\t\t\t<br><b>{{ _(\"SR.No\") }}:</b><br>\n\t\t\t\t\t{{ item.serial_no | replace(\"\\n\", \", \") }}\n\t\t\t\t{%- endif -%}\n\t\t\t</td>\n\t\t\t<td>{{ item.item_pt }}</td>\n\t\t\t<td class=\"text-right\">{{ item.qty }}</td>\n\t\t\t{%- endif -%}\n\t\t</tr>\n\t\t{%- endfor -%}\n\t</tbody>\n</table></center>",
